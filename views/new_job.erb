<form action="/job/create" role="form" method="post">
  <div class="form-group">
    <label for="datepicker">Date</label>
    <input type="text" name="date" class="form-control" id="datepicker" />
  </div>
  <div class="form-group">
    <label for="timepicker">Time (military)</label>
    <input type="text" name="time" class="form-control" id="timepicker" placeholder="HH:MM" />
  </div>
  <div class="form-group">
    <label for="formScript">Command</label>
    <input type="text" name="script" class="form-control" id="formScript" />
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
